{
  "name": "drabina",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "^3.4.1",
    "natives": "^1.1.6",
    "normalize.css": "~8.0.1"
  },
  "devDependencies": {
    "autoprefixer": "~9.7.4",
    "browser-sync": "^2.26.7",
    "browserify": "~16.5.0",
    "del": "~5.1.0",
    "envify": "~4.1.0",
    "foldero": "~0.1.1",
    "glob": "~7.1.6",
    "gulp": "^4.0",
    "gulp-changed": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-data": "~1.3.1",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-if": "~3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-jade": "~1.1.0",
    "gulp-load-plugins": "^2.0.2",
    "gulp-notify": "~3.2.0",
    "gulp-plumber": "~1.2.1",
    "gulp-postcss": "~8.0.0",
    "gulp-rename": "~2.0.0",
    "gulp-sass": "~4.0.2",
    "gulp-sourcemaps": "~2.6.5",
    "gulp-uglify": "~3.0.2",
    "gulp-util": "~3.0.8",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "jade": "~1.11.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "minimist": "~1.2.0",
    "rimraf": "~3.0.0",
    "vinyl-buffer": "~1.0.1",
    "vinyl-source-stream": "~2.0.0",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules",
    "clean": "gulp clean",
    "lint": "gulp eslint",
    "serve:prod": "NODE_ENV=development gulp serve",
    "serve": "NODE_ENV=production gulp serve --production",
    "build": "NODE_ENV=production gulp --production"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
